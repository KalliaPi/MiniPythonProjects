    "    f=(c*1.8)+32.0\n",
    "    return f\n",
    "def printTemp(c,f):\n",
    "        print(f\"temperature of {c:.0f} celsius is equal to {f:.0f} fahrenheit\")\n",
    "c=float(input('enter celsius:'))\n",
    "printTemp(c, c2f(c))"
